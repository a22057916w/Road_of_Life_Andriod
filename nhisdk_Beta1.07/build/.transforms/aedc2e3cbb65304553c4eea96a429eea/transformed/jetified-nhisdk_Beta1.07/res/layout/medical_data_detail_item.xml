<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/nhisdk_data_detail_item_layout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:paddingLeft="@dimen/margin_normal">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_marginTop="@dimen/margin_normal">

        <!--<View-->
            <!--android:layout_width="10dp"-->
            <!--android:layout_height="match_parent"-->
            <!--android:layout_marginLeft="@dimen/margin_normal"-->
            <!--android:background="@android:color/darker_gray"-->
            <!--/>-->
        <RelativeLayout
            android:layout_width="10dp"
            android:layout_height="10dp"
            android:background="@drawable/bg_data_detail_title_icon"
            android:layout_marginLeft="@dimen/margin_normal"
            android:layout_gravity="center_vertical"/>

        <TextView
            android:id="@+id/nhisdk_data_detail_item_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/margin_normal"
            android:textSize="@dimen/common_content_txt_size"
            android:textColor="@color/color_detail_title_blue"/>
    </LinearLayout>

    <TextView
        android:id="@+id/nhisdk_data_detail_item_content"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/margin_normal"
        android:textColor="@android:color/black"
        android:textSize="@dimen/common_content_txt_size"
        android:layout_marginLeft="@dimen/data_production_detail_margin_left"
        android:layout_marginBottom="@dimen/margin_middle"/>


</LinearLayout>